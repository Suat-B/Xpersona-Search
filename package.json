{
  "name": "xpersona-casino",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:fresh": "node -e \"require('fs').rmSync('.next',{recursive:true,force:true}); console.log('Cache cleared')\" && next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "api:inventory": "node scripts/api-inventory.mjs",
    "api:refs": "node scripts/check-endpoint-references.mjs",
    "api:legacy-smoke": "node scripts/check-legacy-api-usage.mjs",
    "check:utf8": "node scripts/check-utf8.mjs",
    "openapi:generate": "node scripts/generate-openapi.mjs",
    "openapi:check": "node scripts/generate-openapi.mjs --check",
    "ci:api": "npm run check:utf8 && npm run api:inventory && npm run api:refs && npm run api:legacy-smoke && npm run openapi:check",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:ans-migrate": "node scripts/ans-db-migrate.mjs",
    "db:ensure-agents-table": "node scripts/ensure-agents-table.mjs",
    "db:ensure-crawler-schema": "node scripts/ensure-crawler-schema.mjs",
    "db:ensure-search-vectors": "node scripts/ensure-search-vectors.mjs",
    "crawl": "tsx scripts/run-crawl.ts",
    "crawl:full": "tsx scripts/run-crawl.ts 2500",
    "crawl:100k": "tsx scripts/run-crawl.ts 100k",
    "crawl:hot": "tsx scripts/run-crawl.ts 500 --mode=hot --github-budget=800 --time-budget-ms=120000",
    "crawl:hot:loop": "node scripts/run-crawl-hot-loop.mjs",
    "crawl:worker": "node scripts/run-crawl-worker.mjs",
    "db:studio": "drizzle-kit studio",
    "setup": "docker compose up -d && npm run db:push",
    "setup:google": "node scripts/setup-google-auth.mjs",
    "setup:stripe": "node scripts/setup-stripe.mjs",
    "setup:ans": "node scripts/setup-ans.mjs",
    "seed": "tsx scripts/seed-packages.ts",
    "seed:marketplace": "tsx scripts/seed-marketplace-strategies.ts",
    "db:backfill-agent-ids": "tsx scripts/backfill-agent-ids.ts",
    "db:backfill-deposits": "tsx scripts/backfill-deposits.ts",
    "db:add-account-type": "tsx scripts/add-account-type-column.ts",
    "db:add-password-hash": "node scripts/add-password-hash.mjs",
    "db:migrate-production": "node scripts/migrate-production.mjs",
    "db:seed-production": "node scripts/seed-production.mjs",
    "repair:claim-integrity": "tsx scripts/repair-claim-developer-integrity.ts",
    "repair:claim-integrity:apply": "tsx scripts/repair-claim-developer-integrity.ts --apply",
    "deploy": "node scripts/deploy.mjs",
    "setup:prod": "node scripts/setup-production.mjs"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.7.2",
    "@octokit/plugin-retry": "^8.1.0",
    "@octokit/plugin-throttling": "^11.0.3",
    "@octokit/rest": "^22.0.1",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.2",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.38.0",
    "gray-matter": "^4.0.3",
    "nanoid": "^5.0.9",
    "next": "^15.0.0",
    "next-auth": "^5.0.0-beta.25",
    "p-limit": "^7.3.0",
    "pyodide": "^0.29.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "resend": "^6.9.2",
    "stripe": "^17.4.0",
    "tailwind-merge": "^3.4.0",
    "yaml": "^2.8.2",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.0.0",
    "@types/pg": "^8.11.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.24",
    "dotenv": "^17.2.4",
    "drizzle-kit": "^0.30.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.0.0",
    "pg": "^8.13.0",
    "postgres": "^3.4.5",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.19.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  }
}
